  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/client/*",
      "src/common/*"
    ],
    "exclude": [
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:icons"
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "jest-junit": {
    "output": "test-results/test-report.junit.xml"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "prettier",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "node_modules/**",
      "lib/**",
      "types/**",
      "src/common/mock-api/**",
      "*.json",
      "**/*.json",
      "src/common/mock-api/**/*.ts"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "tslint --project ./tsconfig.json src/**/*{.ts,.tsx}",
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build --release",
    "build-stats": "yarn run build --release --analyse",
    "deploy": "babel-node tools/run deploy",
    "render": "babel-node tools/run render",
    "serve": "babel-node tools/run runServer",
    "start": "babel-node tools/run start",
    "build-cypress": "babel-node tools/run build --release --cypress",
    "cypress": "cypress open --env $URL",
    "cypress-run": "cypress run --record --key 541c8b23-47a2-47e9-a8e1-ff1d29f6da50 --env $URL",
    "cypress:open": "cypress open"
